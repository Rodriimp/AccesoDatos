package proyecto.modelo;

import java.math.BigDecimal;
import java.math.BigInteger;
import java.time.LocalDate;

public class Registro {

	private Long idRegistro;
	private Long idUsuario;
	private LocalDate fecha;
	private BigDecimal num_horas;
	private String descripcion;

	public BigInteger getId_registro() {
		return idRegistro;
	}

	public void setId_registro(BigInteger id_registro) {
		this.idRegistro = id_registro;
	}

	public BigInteger getId_usuario() {
		return idUsuario;
	}

	public void setId_usuario(BigInteger id_usuario) {
		this.idUsuario = id_usuario;
	}

	public LocalDate getFecha() {
		return fecha;
	}

	public void setFecha(LocalDate fecha) {
		this.fecha = fecha;
	}

	public BigDecimal getNum_horas() {
		return num_horas;
	}

	public void setNum_horas(BigDecimal num_horas) {
		this.num_horas = num_horas;
	}

	public String getDescripcion() {
		return descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

}
